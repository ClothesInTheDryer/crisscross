            <a href="<%= ResolveClientUrl("~") %>Help/HomeHelp.aspx">Help</a>
        </div>
        <div class="menuLink" id="introLinkWrapper" style="display:none;">
            <a id="introLink" href="<%= ResolveClientUrl("~") %>Help/HomeHelp.aspx">Intro</a>
