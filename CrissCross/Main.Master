                        <div class="inner">
        <div style="float:left">
        <h2>
            <asp:Label ID="uxAppTitle" runat="server" Text="CrissCross"></asp:Label></h2>
        </div>
        
        <div class="menuLink">
            <a href="<%= ResolveClientUrl("~") %>Home.aspx">Home</a>
        </div>
        <div class="menuLink">
